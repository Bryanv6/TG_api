<script>
  $('#region').change(function(){
    switch($(this).val()) {
      case 'Africa':
        map.setCenter(new google.maps.LatLng(6.436616, 21.059173));
        map.setZoom(3.5);
        break;
      case 'America':
        map.setCenter(new google.maps.LatLng(17.927639, -83.882233));
        map.setZoom(3);
        break;
      case 'Asia Pacific':
        map.setCenter(new google.maps.LatLng(15.552541, 114.753663));
        map.setZoom(3);
        break;
      case 'Europe':
        map.setCenter(new google.maps.LatLng(47.497167, 9.616146));
        map.setZoom(4);
        break;
      case 'Middle East':
        map.setCenter(new google.maps.LatLng(43.677245, 69.903863));
        map.setZoom(4);
        break;
    }
  });
  var pin = {
    url: "https://developer.salesforce.com/hotsites-assets/trailheadx2018/astro-pin-58a85c9729241c608b465de12d24b3ecdc0bde48d6749ac7ae536460e431e80a.png",
    scaledSize: new google.maps.Size(30, 37),
    anchor: new google.maps.Point(15, 37)
  };
  var studentPin = {
    url: "https://developer.salesforce.com/hotsites-assets/trailheadx2018/astro-student-pin-39341d367a7b2c4fd4ea16d3810ff877401ad64376a3e9bd180947c39ef00469.png",
    scaledSize: new google.maps.Size(30, 35),
    anchor: new google.maps.Point(15, 35)
  };
  var markers=[];
  $('.marker').each(function() {
    if( $(this).attr('data-position') !== '' ){
      var position = $(this).attr('data-position');
      var lat = parseFloat(position.split(' ')[0]);
      var long = parseFloat(position.split(' ')[1]);
      if( $(this).attr('data-student-group') == 'Y' ){
        markers.push([$(this).attr('data-group-name'), lat, long, $(this).html(), studentPin]);
      }else{
        markers.push([$(this).attr('data-group-name'), lat, long, $(this).html(), pin]);
      }

    }
  });

  var map = gmap({
    divId: 'location-map',
    center: {lat: 37.785390, lng: -122.405029},
    zoom: 2,
    markers: markers,
    /*pin: "https://developer.salesforce.com/hotsites-assets/trailheadx2017/location/astro-map-pin-366625eab8c205a7a5353e468a536e4596a26ea66af6036214f87d2e8dbd393e.png",*/
    style: [
      {
        "elementType": "geometry",
        "stylers": [
          {
            "color": "#f5f5f5"
          }
        ]
      },
      {
        "elementType": "labels.icon",
        "stylers": [
          {
            "visibility": "off"
          }
        ]
      },
      {
        "elementType": "labels.text.fill",
        "stylers": [
          {
            "color": "#616161"
          }
        ]
      },
      {
        "elementType": "labels.text.stroke",
        "stylers": [
          {
            "color": "#f5f5f5"
          }
        ]
      },
      {
        "featureType": "administrative.land_parcel",
        "stylers": [
          {
            "visibility": "off"
          }
        ]
      },
      {
        "featureType": "administrative.land_parcel",
        "elementType": "labels.text.fill",
        "stylers": [
          {
            "color": "#bdbdbd"
          }
        ]
      },
      {
        "featureType": "administrative.neighborhood",
        "stylers": [
          {
            "visibility": "off"
          }
        ]
      },
      {
        "featureType": "poi",
        "elementType": "geometry",
        "stylers": [
          {
            "color": "#eeeeee"
          }
        ]
      },
      {
        "featureType": "poi",
        "elementType": "labels.text",
        "stylers": [
          {
            "visibility": "off"
          }
        ]
      },
      {
        "featureType": "poi",
        "elementType": "labels.text.fill",
        "stylers": [
          {
            "color": "#757575"
          }
        ]
      },
